  # Pods for ChartDemo

  target 'ChartDemoTests' do
    inherit! :search_paths
    use_frameworks!
    pod 'Charts'
    pod 'RealmSwift'
    pod 'Alamofire' #, '~> 5.0.0-beta.6'
    pod 'SwiftyJSON'
    # Pods for testing
  end

  target 'ChartDemoUITests' do
    inherit! :search_paths
    use_frameworks!
    pod 'Charts'
    pod 'RealmSwift'
    pod 'Alamofire' #, '~> 5.0.0-beta.6'
    pod 'SwiftyJSON'
    # Pods for testing
  end

end
